<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="109dp" android:height="32dp" android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#ffe1e5f0" />
            <solid android:color="#F4F7FF" />
            <corners android:radius="16dp" />
        </shape>
    </item>

    <item android:width="109dp" android:height="32dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#ffe1e5f0" />
            <solid android:color="#ffffffff" />
            <corners android:radius="16dp" />
        </shape>
    </item>
</selector>