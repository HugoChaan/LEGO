<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fu_nav_graph"
    app:startDestination="@id/homeFragment"
    >

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.faceunity.app_ptag.ui.home.FuDemoHomeFragment"
        >
        <argument
            android:name="avatarId"
            app:argType="string"
            />
    </fragment>

    <fragment
        android:id="@+id/editFragment"
        android:name="com.faceunity.app_ptag.ui.edit.FuDemoEditFragment"
        />

    <fragment
        android:id="@+id/driveFragment"
        android:name="com.faceunity.app_ptag.ui.drive.FuDemoDriveFragment"
        />

    <fragment
        android:id="@+id/buildFragment"
        android:name="com.faceunity.app_ptag.ui.build_avatar.FuDemoBuildFragment"
        />

    <fragment
        android:id="@+id/interactionFragment"
        android:name="com.faceunity.app_ptag.ui.interaction.InteractionFragment"
        />
    <fragment
        android:id="@+id/functionDemoFragment"
        android:name="com.faceunity.app_ptag.ui.function_demo.FunctionDemoFragment"
        />

    <fragment
        android:id="@+id/scanCodeFragment"
        android:name="com.faceunity.app_ptag.ui.scan_code.FuScanCodeFragment"
        />

</navigation>